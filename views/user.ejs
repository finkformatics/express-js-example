<%- include('partials/header.ejs') %>

<!-- Render the user data here -->
<h5 class="card-title">User</h5>
<div class="row mt-3">
    <div class="col"><b>Name:</b></div>
    <div class="col"><%= registeredUser.name %></div>
</div>
<div class="row">
    <div class="col"><b>E-Mail:</b></div>
    <div class="col"><%= registeredUser.email %></div>
</div>
<div class="row mt-3">
    <div class="col">
        <!-- Render a link to /unregister/:email -->
        <a href="/unregister/<%= registeredUser.email %>" class="btn btn-danger">Unregister</a>
    </div>
</div>
                  
<%- include('partials/footer.ejs') %>